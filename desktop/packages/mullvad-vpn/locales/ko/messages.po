msgid "Add <b>%(locationType)s</b> to list"
msgstr "목록에 <b>%(locationType)s</b> 추가"

msgctxt "select-location-view"
msgid "All locations"
msgstr "모든 위치"

msgctxt "select-location-view"
msgid "City"
msgstr "도시"

msgctxt "select-location-view"
msgid "Country"
msgstr "국가"

msgctxt "select-location-view"
msgid "Custom bridge"
msgstr "사용자 지정 브리지"

msgctxt "select-location-view"
msgid "Custom lists"
msgstr "사용자 지정 목록"

msgctxt "select-location-view"
msgid "Do you want to delete the list <b>%(list)s</b>?"
msgstr "<b>%(list)s</b> 목록을 삭제하시겠습니까?"

msgctxt "select-location-view"
msgid "Edit list name"
msgstr "목록 이름 수정"

