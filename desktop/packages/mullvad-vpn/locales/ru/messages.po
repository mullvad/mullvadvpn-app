msgctxt "login-view"
msgid "Invalid account number"
msgstr "Недействительный номер учетной записи"

msgctxt "login-view"
msgid "Logged in"
msgstr "Вход выполнен"

msgctxt "login-view"
msgid "Logging in..."
msgstr "Выполняется вход..."

#. Label for the login button.
msgctxt "login-view"
msgid "Login"
msgstr "Вход"

msgctxt "login-view"
msgid "Login failed"
msgstr "Ошибка входа"

