msgid "%(tunnelType)s via %(bridgeType)s"
msgstr "%(bridgeType)s aracılığıyla %(tunnelType)s"

msgctxt "connection-info"
msgid "In"
msgstr "Giriş"

msgctxt "connection-info"
msgid "Out"
msgstr "Çıkış"

