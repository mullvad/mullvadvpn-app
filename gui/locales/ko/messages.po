msgstr "계정 생성"

msgctxt "login-view"
msgid "Creating account..."
msgstr "계정 생성 중..."

msgctxt "login-view"
msgid "Don’t have an account number?"
msgstr "계정 번호가 없으신가요?"

msgctxt "login-view"
msgid "Enter your account number"
msgstr "계정 번호 입력"

msgctxt "login-view"
msgid "Error"
msgstr "오류"

msgctxt "login-view"
