msgctxt "filter-view"
msgid "All providers"
msgstr "모든 제공업체"

msgctxt "filter-view"
msgid "Mullvad owned only"
msgstr "Mullvad 소유만"

msgctxt "filter-view"
msgid "Owned"
msgstr "소유"

msgctxt "filter-view"
msgid "Ownership"
msgstr "소유권"

msgctxt "filter-view"
msgid "Providers"
msgstr "제공업체"

msgctxt "filter-view"
msgid "Rented"
msgstr "대여"

msgctxt "filter-view"
msgid "Rented only"
