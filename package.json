  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.23.6",
